{
	"publisher": "appland",
	"name": "appmap",
	"displayName": "AppMap",
	"description": "Show AppMap diagram for context object",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:appland.scenarioFile"
	],
	"license": "MIT",
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "appland.scenarioFile",
				"displayName": "AppLand Scenario",
				"selector": [
					{
						"filenamePattern": "*.appmap.json"
					}
				],
				"priority": "default"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js && node ./tape/**-test.js",
		"tape": "node ./tape/**-test.js",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"tape": "^5.0.1"
	},
	"dependencies": {
		"@appland/diagrams": "^0.4.2",
		"@appland/models": "^0.3.0",
		"bootstrap": "^4.5.3",
		"bootstrap-autocomplete": "^2.3.7",
		"d3": "^6.3.1",
		"jquery": "^3.5.1",
		"popper": "^1.0.1"
	}
}
